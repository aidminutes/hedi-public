{
  "name": "hedi-app",
  "version": "0.5.0",
  "description": "HEDI App - web server and client",
  "author": "aidminutes GmBH",
  "scripts": {
    "view": "yarn install && yarn compileCSS && yarn next:dev",
    "compileCSS": "yarn gulp scss",
    "next:dev": "next dev",
    "dev": "yarn compileCSS && concurrently \"yarn watch:scss\" \"yarn next:dev\" ",
    "debug": "yarn compileCSS && yarn next:dev",
    "next:build": "next build --debug",
    "build": "yarn install && yarn compileCSS && yarn preBuild && yarn next telemetry disable && ((yarn next:build && yarn postBuild) || yarn postBuild)",
    "preBuild": "node ./src/tasks/preBuild/index.js",
    "postBuild": "node ./src/tasks/postBuild/index.js",
    "start": "next start",
    "pretty": "prettier --write './**/*.ts*'",
    "compileTS": "tsc --version && tsc --pretty --noEmit --noErrorTruncation",
    "watch:scss": "yarn gulp",
    "test": "jest",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@carbon/icons": "^10.48.0",
    "@carbon/icons-react": "^10.48.0",
    "@webcomponents/shadydom": "^1.7.4",
    "camelcase": "^6.2.0",
    "carbon-components": "10.55.2",
    "carbon-components-react": "7.55.2",
    "carbon-icons": "7.0.7",
    "classnames": "^2.3.1",
    "core-js": "^3.19.1",
    "dompurify": "2.3.3",
    "graphql": "^15.4.0",
    "graphql-request": "^3.3.0",
    "htmlparser2": "^6.0.1",
    "leaflet": "^1.7.1",
    "leaflet-defaulticon-compatibility": "^0.1.1",
    "link-preview-js": "2.1.8",
    "matrix-js-sdk": "^20.1.0",
    "next": "12.1.0",
    "next-auth": "4.0.2",
    "next-transpile-modules": "^9.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "^3.0.5",
    "regenerator-runtime": "^0.13.9",
    "sharp": "^0.30.5",
    "swr": "1.1.0",
    "uuid": "^8.3.2",
    "video.js": "^7.14.3"
  },
  "devDependencies": {
    "@playwright/test": "^1.21.1",
    "@types/carbon-components-react": "^7.49.0",
    "@types/carbon__icons-react": "^10.31.2",
    "@types/dompurify": "2.3.1",
    "@types/htmlparser2": "^3.10.2",
    "@types/jest": "^27.0.3",
    "@types/leaflet": "^1.5.21",
    "@types/node": "14.14.5",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.2",
    "@types/react-leaflet": "^2.5.2",
    "@types/uuid": "^8.3.3",
    "@types/video.js": "^7.3.12",
    "autoprefixer": "^9.7.0",
    "concurrently": "6.2.0",
    "fetch-mock-jest": "^1.5.1",
    "fs-readdir-recursive": "^1.1.0",
    "git-format-staged": "^2.1.0",
    "gulp": "4.0.2",
    "gulp-postcss": "8.0.0",
    "gulp-sass": "^5.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "husky": "^8.0.1",
    "jest": "^27.4.4",
    "node-sass": "^7.0.0",
    "prettier": "^2.2.0",
    "typescript": "4.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git-format-staged -f 'prettier --ignore-unknown --stdin-filepath \"{}\"' '*.ts' '*.tsx'"
    }
  }
}
